<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>latest</LangVersion>
		<PackageProjectUrl>https://www.gizmopowered.net/</PackageProjectUrl>
		<Authors>NETProjects</Authors>
		<Product>Gizmo</Product>
		<Description>Gizmo web api client. This library contains web api clients implementations.</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/GAMP/Gizmo.Web.Api.Client</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<Copyright>NETProjects</Copyright>
		<PackageIcon>package_icon.png</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="MessagePack" Version="2.2.85" />
		<PackageReference Include="Microsoft.Extensions.Http" Version="5.0.0" />
		<PackageReference Include="System.Net.Http.Json" Version="5.0.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Gizmo.Shared\Gizmo.Shared.csproj" />
		<ProjectReference Include="..\Gizmo.Web.Api.Models\Gizmo.Web.Api.Models.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="Resources\package_icon.png">
	    <Pack>True</Pack>
	    <PackagePath></PackagePath>
	  </None>
	</ItemGroup>

</Project>
